{
  "name": "react-chat-windower",
  "version": "1.2.1",
  "description": "A clone of the react-chat-window React component with enhancements",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component --copy-files",
    "clean": "nwb clean-module && nwb clean-demo",
