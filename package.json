    "start": "nwb serve-react-demo",
    "gh:publish": "nwb build-demo && gh-pages -d demo/dist"
  },
  "dependencies": {
    "emoji-js": "3.2.2",
    "gh-pages": "^1.2.0",
    "prop-types": "15.5.10",
    "react-highlight.js": "1.0.5",
    "react-linkify": "^0.2.1",
    "socket.io-client": "2.0.3"
  },
  "peerDependencies": {
    "react": "15.x"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.13.0",
    "eslint-plugin-react": "^7.12.4",
    "nwb": "^0.23.0",
    "react": "15.6.1",
    "react-dom": "15.6.1"
  },
  "author": "kingofthestack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rburton/react-chat-window.git"
